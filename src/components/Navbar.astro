---
const links = [
	{ label: 'Inicio', href: '#top' },
	{ label: 'Experiencia', href: '#experiencia' },
	{ label: 'Sobre mi', href: '#sobre-mi' },
	{ label: 'Proyectos', href: '#proyectos' },
];
---

{
	links.map(link => {
		return (
			<li>
				<a href={link.href} class="font-medium capitalize hover:text-accent">
					{link.label}
				</a>
			</li>
		);
	})
}
